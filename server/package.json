{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run live",
    "serve": "npm run build && node ./build/index.js",
    "build": "rm -rf ./build/* && npm run tslint && tsc ",
    "live:node": "nodemon ./build/index.js",
    "live:ts": "tsc -w",
    "live": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\"  \"npm run live:ts\" \"npm run live:node\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.10.0",
    "dotenv": "^8.1.0",
    "ioredis": "^4.14.0",
    "normalize-url": "^4.3.0",
    "ramda": "^0.26.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/bull": "^3.10.2",
    "@types/dotenv": "^6.1.1",
    "@types/ioredis": "^4.0.15",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/ramda": "^0.26.21",
    "concurrently": "^4.1.2",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  }
}
